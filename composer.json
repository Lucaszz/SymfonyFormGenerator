{
    "name": "lucaszz/symfony-form-generator",
    "description": "Generic solution to create forms \"on the fly\" basing on class metadata",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "lucaszz",
            "email": "lucaszz111@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Lucaszz\\SymfonyFormGenerator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4" : { "Lucaszz\\SymfonyFormGenerator\\Tests\\" : "tests" }
    },
    "require": {
        "php": ">=5.5",
        "symfony/form": "~2.3",
        "symfony/validator": "~2.3",
        "doctrine/annotations": "~1.0",
        "phpdocumentor/reflection-docblock": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.8",
        "ramsey/uuid": "~3.0",
        "mathiasverraes/money": "~1.0",
        "symfony/symfony": "~2.3"
    }
}
