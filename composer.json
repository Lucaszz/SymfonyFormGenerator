{
    "name": "lucaszz/symfony-generic-form",
    "description": "Generic solution to create forms \"on the fly\" basing on class metadata",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "lucaszz",
            "email": "lucaszz111@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Lucaszz\\SymfonyGenericForm\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4" : { "Lucaszz\\SymfonyGenericForm\\Tests\\" : "tests" }
    },
    "require": {
        "php": ">=5.5",
        "symfony/form": "~2.3"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.8",
        "ramsey/uuid": "~3.0",
        "symfony/symfony": "~2.3",
        "sensio/generator-bundle": "~2.3"
    }
}
